<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.androidgametictactoe">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="Tic Tac Toe"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ThreeByThreeSinglePlayer"
            android:screenOrientation="portrait"/>
        <activity android:name=".ThreeByThreeTwoPlayers"
            android:screenOrientation="portrait"/>
        <activity android:name=".FiveByFiveSinglePlayer"
            android:screenOrientation="portrait"/>
        <activity android:name=".FiveByFiveTwoPlayers"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>